me) =>{\r\n            totalIncome = totalIncome + income.amount\r\n        })\r\n\r\n        return totalIncome;\r\n    }\r\n\r\n\r\n    const totalBalance = () => {\r\n        return totalIncome() - totalExpenses()\r\n    }\r\n\r\n    const transactionHistory = () => {\r\n        const history = [...incomes, ...expenses]\r\n        history.sort((a, b) => {\r\n            return new Date(b.createdAt) - new Date(a.createdAt)\r\n        })\r\n\r\n        return history.slice(0, 3)\r\n    }\r\n\r\n\r\n    return (\r\n        <GlobalContext.Provider value={{\r\n            addIncome,\r\n            getIncomes,\r\n            incomes,\r\n            deleteIncome,\r\n            expenses,\r\n            totalIncome,\r\n            addExpense,\r\n            getExpenses,\r\n            deleteExpense,\r\n            totalExpenses,\r\n            totalBalance,\r\n            transactionHistory,\r\n            error,\r\n            setError\r\n        }}>\r\n            {children}\r\n        </GlobalContext.Provider>\r\n    )\r\n}\r\n\r\nexport const useGlobalContext = () =>{\r\n    return useContext(GlobalContext)\r\n}",[],"C:\\Users\\Usuario\\Desktop\\expense tracker\\expense-tracker_fullstack\\frontend\\src\\styles\\GlobalStyle.js",[],[],"C:\\Users\\Usuario\\Desktop\\expense tracker\\expense-tracker_fullstack\\frontend\\src\\styles\\Layouts.js",[],[],"C:\\Users\\Usuario\\Desktop\\expense tracker\\expense-tracker_fullstack\\frontend\\src\\Components\\Navigation\\Navigation.js",["144"],[],"C:\\Users\\Usuario\\Desktop\\expense tracker\\expense-tracker_fullstack\\frontend\\src\\Components\\Orb\\Orb.js",[],[],"C:\\Users\\Usuario\\Desktop\\expense tracker\\expense-tracker_fullstack\\frontend\\src\\Components\\Expenses\\Expenses.js",["145","146","147"],[],"import React, { useEffect } from 'react'\r\nimport styled from 'styled-components'\r\nimport { useGlobalContext } from '../../context/globalContext';\r\nimport { InnerLayout } from '../../styles/Layouts';\r\nimport Form from '